  <% if @model == "user" %>
    <h2>Users search for '<%= @value %>'</h2>
    <table>
        <thead>
            <tr>
                <th></th>
                <th>Name</th>
                <th>Introduction</th>
            </tr>
        </thead>
    <% @datas.each do |user| %>
        <tbody>
            <tr>
                <th>
                    <%= attachment_image_tag(user, :profile_image, :fill, 40, 40, fallback: "img/no_image.jpg", size:'40x40') %>
                </th>
                <th>
                    <%= user.name %>
                </th>
                <th>
                    <%= user.introduction %>
                </th>
            </tr>
        </tbody>
    <% end %>
  <% elsif @model == "post" %>
    <h2>Posts search for '<%= @value %>'</h2>
    <table>
        <thead>
            <tr>
                <th></th>
                <th>board name</th>
                <th>genre</th>
            </tr>
        </thead>
        <% @datas.each do |post| %>
        <tbody>
            <tr>
                <th>
                    <%= attachment_image_tag(post.user, :profile_image, :fill, 40, 40, fallback: "img/no_image.jpg", size:'40x40') %>
                </th>
                <th>
                    <%= post.title %>
                </th>
                <th>
                    <%= post.genre %>
                </th>
            </tr>
        </tbody>
        <% end %>
    <% end %>
    </table>
  </div>
</div>
