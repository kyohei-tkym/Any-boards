<% if @model == "user" %>
<div class="container">
    <div class="user-container">
        <div class="user-card">
            <h2 class="user-title">Users search for '<%= @value %>'</h2>
            <%= render 'relationships/users', users: @datas %>
        </div>
    </div>
</div>
<% elsif @model == "post" %>
    <h2 class="search-title">Search for '<%= @value %>'</h2>
    <%= render 'posts/post-index', posts: @datas %>
<% else %>
    <h2 class="search-title">Search for '<%= @value %>'</h2>
    <%= render 'posts/post-index', posts: @datas %>
<% end %>
    </table>
  </div>
</div>
